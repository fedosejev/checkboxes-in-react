{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "env": {
    "node": true
  },
  "rules": {
    "arrow-parens": ["error", "as-needed"],
    "no-console": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0,
    "no-return-assign": 0,
    "jsx-a11y/label-has-for": 0,
  }
}
